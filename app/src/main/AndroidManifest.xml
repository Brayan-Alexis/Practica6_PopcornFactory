<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fajardo.brayan.popcornfactory">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PopCornFactory">
        <activity android:name="fajardo.brayan.popcornfactory.menu"></activity>
        <activity android:name="fajardo.brayan.popcornfactory.DetallePelicula" />
        <activity android:name="fajardo.brayan.popcornfactory.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>